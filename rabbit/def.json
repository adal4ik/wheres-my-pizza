{
    "users": [
      {
        "name": "guest",
        "password": "guest",
        "tags": "administrator"
      }
    ],
    "vhosts": [
      { "name": "/" }
    ],
    "permissions": [
      {
        "user": "guest",
        "vhost": "/",
        "configure": ".*",
        "write": ".*",
        "read": ".*"
      }
    ],
    "exchanges": [
      {
        "name": "orders_topic",
        "vhost": "/",
        "type": "topic",
        "durable": true,
        "auto_delete": false,
        "internal": false,
        "arguments": {}
      }
    ],
    "queues": [
      {
        "name": "orders_queue",
        "vhost": "/",
        "durable": true,
        "auto_delete": false,
        "arguments": {
          "x-max-priority": 10
        }
      }
    ],
    "bindings": [
      {
        "source": "orders_topic",
        "vhost": "/",
        "destination": "orders_queue",
        "destination_type": "queue",
        "routing_key": "kitchen.*.*",
        "arguments": {}
      }
    ]
  }
  